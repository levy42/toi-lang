import time
import os
import string

timeit = {
  n = 0
  _startime = 0

  __enter = fn(self)
    self._startime = os.clock()
    print "started"

  __exit = fn(self, ex)
    elapsed = os.clock() - self._startime
    print "finished " + string(self.n) + " in " + string(elapsed) + "s"
}


