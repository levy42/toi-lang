from lib.test import assert_eq

sum = 0
for v in {1, 2, 3}
  doubled = v * 2
  sum = sum + doubled
assert_eq(sum, 12)

fn classify(n)
  if n == 2
    return true
  return false

fn dispatch(values)
  for v in values
    ok = classify(v)
    if ok
      return 'ok'
  return 'no'

assert_eq(dispatch({1, 2, 3}), 'ok')
print 'for loop locals ok'
